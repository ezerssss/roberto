{
  "manifest_version": 3,
  "name": "Whats Up",
  "version": "0.1.0",
  "description": "A friendly chat bot that checks up on you, and monitors your Moodle assignments.",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "host_permissions": ["https://*/"],
  "permissions": ["alarms"],
  "background": {
    "service_worker": "background.js"
  }
}
